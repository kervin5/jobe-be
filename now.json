{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@now/node@canary",
      "config": {
        "maxLambdaSize": "25mb"
      }
    }
  ],
  "build": {
    "env": {
      "POSTGRESQL_URL": "@postgresql_url"
    }
  },
  "routes": [
    {
      "src": "/graphql",
      "dest": "src/server.ts"
    }
  ],
  "env": {
    "POSTGRESQL_URL": "@postgresql_url"
  }
}
